<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/blackbackground" />

    <ImageView
        android:id="@+id/cs125Logo"
        android:layout_width="287dp"
        android:layout_height="300dp"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginBottom="268dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/cs125" />

    <TextView
        android:id="@+id/gameVitoryBot"
        android:layout_width="282dp"
        android:layout_height="112dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="480dp"
        android:autoText="false"
        android:bufferType="normal"
        android:capitalize="none"
        android:fontFamily="casual"
        android:lineSpacingExtra="8sp"
        android:text="@string/gameVictoryBot"
        android:textColor="#E10B0B"
        android:textSize="30sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/gameVitoryTop"
        android:layout_width="252dp"
        android:layout_height="42dp"
        android:layout_marginStart="80dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="100dp"
        android:autoText="false"
        android:bufferType="normal"
        android:capitalize="none"
        android:fontFamily="casual"
        android:lineSpacingExtra="8sp"
        android:text="@string/gameVictoryTop"
        android:textColor="#E10B0B"
        android:textSize="30sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/gOverBotGeoff"
        android:layout_width="302dp"
        android:layout_height="129dp"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="460dp"
        android:fontFamily="casual"
        android:lineSpacingExtra="8sp"
        android:text="@string/gameOverBotGeoff"
        android:textColor="#E10B0B"
        android:textSize="30sp"
        android:textStyle="bold"
        android:typeface="normal"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="invisible" />

    <ImageView
        android:id="@+id/gameOverGeoff"
        android:layout_width="259dp"
        android:layout_height="251dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:layout_marginBottom="292dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/error"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/gOverTopGeoff"
        android:layout_width="300dp"
        android:layout_height="59dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="88dp"
        android:autoText="false"
        android:bufferType="normal"
        android:capitalize="none"
        android:fontFamily="casual"
        android:gravity="center"
        android:lineSpacingExtra="8sp"
        android:text="@string/gameOverTopGeoff"
        android:textColor="#E10B0B"
        android:textSize="30sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="invisible" />

    <Button
        android:id="@+id/toMenu"
        android:layout_width="365dp"
        android:layout_height="77dp"
        android:layout_marginStart="31dp"
        android:layout_marginLeft="31dp"
        android:layout_marginBottom="33dp"
        android:text="Back to Start"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>